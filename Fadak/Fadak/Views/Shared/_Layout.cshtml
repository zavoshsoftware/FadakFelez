    <script type="text/javascript">
$(document).ready(function () {
    var URL = window.location.pathname;
    if (URL.includes('BlogCategories')||URL.includes('Blogs')) {
        $('.menuitem').removeClass('active');
        $('#blogLi').addClass('active');
    }
    else if (URL.includes('TourCategories') || URL.includes('Toures') || URL.includes('IncludeItems')
        || URL.includes('OptionalServices')
        || URL.includes('Rel_Tours')
        || URL.includes('TourImages')
        || URL.includes('TourDay')


        ) {
        $('.menuitem').removeClass('active');
        $('#touresLi').addClass('active');
    }
    else if (URL.includes('Cities')) {
        $('.menuitem').removeClass('active');
        $('#cityLi').addClass('active');
    }
    else if (URL.includes('Galler')) {
        $('.menuitem').removeClass('active');
        $('#galleryLi').addClass('active');
    }
    else if (URL.includes('/TextItems/index/1') || URL.includes('TeamMembers') || URL.includes('Services')){
        $('.menuitem').removeClass('active');
        $('#aboutLi').addClass('active');
    }
    else if (URL.includes('/TextItems/index/2') || URL.includes('Messages')) {
        $('.menuitem').removeClass('active');
        $('#contactLi').addClass('active');
    }

    else if (URL.includes('/TextItems/index/3') || URL.includes('Feedbacks')) {
        $('.menuitem').removeClass('active');
        $('#homeLi').addClass('active');
    }
    else if (URL.includes('/NewsLetters')) {
        $('.menuitem').removeClass('active');
        $('#newsletterLi').addClass('active');
    }
});
    </script>


